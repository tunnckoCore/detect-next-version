{
  "name": "detect-next-version",
  "description": "Calculates next version, based on given commit message and following Conventional Commits",
  "license": "Apache-2.0",
  "licenseStart": "2017",
  "scripts": {
    "start": "tunnckocore-scripts",
    "extends": "@tunnckocore/config/scripts",
    "docs": "tunnckocore-scripts docs",
    "lint": "tunnckocore-scripts lint",
    "test": "tunnckocore-scripts test",
    "commit": "tunnckocore-scripts commit"
  },
  "engines": {
    "node": "^8.11.0 || >=10.13.0"
  },
  "dependencies": {
    "@tunnckocore/package-json": "^1.0.1",
    "esm": "^3.0.84",
    "parse-commit-message": "3.2.0",
    "recommended-bump": "^1.2.0"
  },
  "devDependencies": {
    "@tunnckocore/config": "^0.5.1",
    "@tunnckocore/scripts": "^1.1.0",
    "asia": "^0.19.7"
  },
  "files": [
    "src",
    "index.js"
  ],
  "main": "index.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "version": "3.0.0",
  "repository": "tunnckoCoreLabs/detect-next-version",
  "homepage": "https://github.com/tunnckoCoreLabs/detect-next-version",
  "author": "Charlike Mike Reagent (https://tunnckocore.com)",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "renovate": {
    "extends": "tunnckocore"
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4,
      "footer": ""
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "execa",
        "charlike",
        "gitcommit",
        "@tunnckocore/create-github-repo",
        "@tunnckocore/create-project",
        "@tunnckocore/package-json",
        "@tunnckocore/execa",
        "@tunnckocore/update",
        "@tunnckocore/scripts",
        "@tunnckocore/config",
        "recommended-bump"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "new-release",
      "semantic-release",
      "execa",
      "parse-commit-message",
      "recommended-bump"
    ]
  }
}
