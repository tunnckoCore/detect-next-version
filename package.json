{
  "name": "detect-next-version",
  "version": "0.0.0-semantic-release-app",
  "description": "Calculates next version, based on given commit message and following Conventional Commits",
  "repository": "tunnckoCore/detect-next-version",
  "author": "Charlike Mike Reagent (https://i.am.charlike.online)",
  "nspId": "d0aea59c-0e09-42ac-bd67-2c48f22c5226",
  "main": "./src/index.js",
  "scripts": {
    "test": "NODE_ENV=test yarn hela test",
    "precommit": "yarn hela precommit",
    "commit": "yarn hela commit",
    "prepublishOnly": "yarn hela protect"
  },
  "license": "Apache-2.0",
  "licenseStart": 2017,
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "files": [
    "src/"
  ],
  "dependencies": {
    "parse-commit-message": "2.1.3"
  },
  "devDependencies": {
    "eslint": "5.2.0",
    "eslint-config-tunnckocore": "1.0.1",
    "hela": "2.0.10",
    "hela-config-tunnckocore": "3.0.26",
    "mukla": "0.4.9"
  },
  "keywords": [
    "automation"
  ],
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "dush",
        "p-map-series",
        "execa",
        "hela",
        "gitcommit",
        "parse-git-log",
        "parse-commit-message",
        "mri"
      ],
      "highlight": "hela"
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "hela",
      "parse-commit-message",
      "semver"
    ]
  }
}
