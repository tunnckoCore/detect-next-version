{
  "name": "detect-next-version",
  "description": "Calculates next version, based on given commit message and following Conventional Commits",
  "license": "Apache-2.0",
  "licenseStart": "2017",
  "scripts": {
    "start": "tunnckocore-scripts",
    "extends": "@tunnckocore/config/scripts",
    "docs": "tunnckocore-scripts docs",
    "lint": "tunnckocore-scripts lint",
    "test": "tunnckocore-scripts test",
    "commit": "tunnckocore-scripts commit"
  },
  "engines": {
    "node": "^8.11.0 || >=10.13.0"
  },
  "dependencies": {
    "@tunnckocore/package-json": "^1.0.1",
    "esm": "^3.0.84",
    "parse-commit-message": "1.1.2",
    "recommended-bump": "^1.0.1"
  },
  "devDependencies": {
    "@tunnckocore/config": "^0.5.1",
    "@tunnckocore/scripts": "^1.1.0",
    "asia": "^0.19.7"
  },
  "files": [
    "src",
    "index.js"
  ],
  "main": "index.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "version": "3.0.0-next.rc5",
  "repository": "tunnckoCoreLabs/detect-next-version",
  "homepage": "https://github.com/tunnckoCoreLabs/detect-next-version",
  "author": "Charlike Mike Reagent (https://tunnckocore.com)",
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "renovate": {
    "extends": "tunnckocore"
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "dush",
        "p-map-series",
        "execa",
        "hela",
        "gitcommit",
        "parse-git-log",
        "parse-commit-message",
        "mri"
      ],
      "highlight": "hela"
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "new-release",
      "semantic-release"
    ]
  }
}
